{
  "name": "rise-of-kingdoms-services",
  "version": "1.0.0",
  "description": "Website cung cấp dịch vụ cho game Rise of Kingdoms",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "build:prod": "NEXT_DISABLE_ESLINT=true prisma generate && next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:strict": "npm run lint -- --max-warnings=0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint:strict && npm run format:check",
    "pre-commit": "npm run validate && npm run test:ci",
    "quality": "bash scripts/code-quality.sh",
    "quality:fix": "npm run lint:fix && npm run format",
    "build:analyze": "ANALYZE=true next build",
    "export": "next export",
    "deploy:vercel": "vercel --prod",
    "deploy:preview": "vercel",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "echo 'Seed disabled for deployment'",
    "db:reset": "prisma migrate reset",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:update": "jest --updateSnapshot",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "security:check": "npm audit --audit-level=moderate",
    "security:full": "bash scripts/security-check.sh",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:all": "npm run test && npm run test:e2e",
    "playwright:install": "playwright install",
    "ws:dev": "tsx watch src/websocket-server.ts",
    "ws:start": "tsx src/websocket-server.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run ws:dev\""
  },
  "keywords": [
    "rise-of-kingdoms",
    "gaming",
    "services",
    "strategy-game"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "3.864.0",
    "@aws-sdk/s3-request-presigner": "3.864.0",
    "@heroicons/react": "2.2.0",
    "@hookform/resolvers": "5.2.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.12.0",
    "@sentry/nextjs": "10.1.0",
    "@supabase/supabase-js": "2.55.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.1",
    "@types/multer": "2.0.0",
    "@types/node": "20.19.9",
    "@types/qrcode": "1.5.5",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/speakeasy": "2.0.10",
    "@upstash/redis": "^1.25.1",
    "autoprefixer": "^10.4.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.0.0",
    "crypto-js": "^4.2.0",
    "date-fns": "4.1.0",
    "discord.js": "^14.14.1",
    "lucide-react": "0.536.0",
    "multer": "2.0.2",
    "multer-s3": "3.0.1",
    "next": "^14.0.0",
    "next-auth": "^4.24.5",
    "otplib": "12.0.1",
    "pg": "8.16.3",
    "postcss": "^8.4.0",
    "qrcode": "1.5.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "7.62.0",
    "resend": "4.8.0",
    "socket.io": "4.8.1",
    "socket.io-client": "4.8.1",
    "speakeasy": "2.0.0",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "1.54.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "9.0.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "9.2.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsonwebtoken": "9.0.2",
    "lint-staged": "^15.0.0",
    "node-fetch": "^3.3.2",
    "node-mocks-http": "^1.13.0",
    "playwright": "1.54.2",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "prisma": "^6.12.0",
    "sharp": "0.34.2"
  }
}
